<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" 
    xmlns:p="urn:import:pro.softcom.archetype.gwt.client.lib.panel">

    <g:VerticalPanel>
        <g:FlowPanel ui:field="fieldsPanelContainer" />
        <g:HorizontalPanel>
            <g:Button ui:field="newButton" text="New" width="80px" />
            <g:Button ui:field="editButton" text="Edit" width="80px" />
            <g:Button ui:field="saveButton" text="Save" width="80px" />
            <g:Button ui:field="deleteButton" text="Delete" width="80px" />
            <g:Button ui:field="cancelButton" text="Cancel" width="80px" />
        </g:HorizontalPanel>
    </g:VerticalPanel>

</ui:UiBinder>